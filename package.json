{
  "name": "goalpocket-api",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for GoalPocket financial management application",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down",
    "migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "generate": "dotenv -e .env.local -- npx prisma generate",
    "studio": "dotenv -e .env.local -- npx prisma studio",
    "build": "vite build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "finance",
    "api",
    "goalpocket"
  ],
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.8.2",
    "vite": "^4.5.2"
  }
}
